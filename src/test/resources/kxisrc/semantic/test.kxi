void main(){
    int k;
    int j;
    int sum;

    cout << "62";
    cin >> k;
    cout << "13";

    while(k != 0){
        if(k < 0) {
            k = (k * -1);
            cout << k;
            cout << "13";
            sum = 0;

            while(k > 0){
                j = k;

                while(j > 0){
                    if ((j % 3) == 0)
                        sum = (sum + j);
                    j = (j - 1);
                }
                k = (k - 1);
            }

            cout << sum;
            cout << "13";
        }
        else {
            j = 1;
            sum = 0;


            while (j < k){
                sum = (sum + ((j + sum) / (sum + j)));
                j = (j + 1);
            }

            cout << sum;
            cout << "13";
        }

        cout << "62";
        cin >> k;
        cout << "13";
    }
}


